{% if route.is_all %}
def {{ route.name }}(db=Depends(get_db)):
    db.query({{ name }}).delete()
    db.commit()
    return {"message": "deleted"}

{% elif just_one_pk %}
def {{ route.name }}(ident: {{ pk_type }}, db=Depends(get_db)):
    db.delete({{ name }}, ident)
    db.commit()
    return {"message": "deleted"}

{% endif %}